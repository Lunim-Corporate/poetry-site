{
  "id": "faq",
  "type": "SharedSlice",
  "name": "Faq",
  "description": "Frequently asked questions accordion",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default FAQ accordion",
      "imageUrl": "",
      "primary": {
        "title": {
          "type": "StructuredText",
          "config": {
            "label": "Title",
            "placeholder": "FAQ",
            "allowTargetBlank": false,
            "single": "heading2"
          }
        },
        "items": {
          "type": "Group",
          "config": {
            "label": "FAQ Items",
            "fields": {
              "question": {
                "type": "Text",
                "config": {
                  "label": "Question",
                  "placeholder": "Enter question..."
                }
              },
              "answer": {
                "type": "StructuredText",
                "config": {
                  "label": "Answer",
                  "placeholder": "Enter answer...",
                  "allowTargetBlank": true,
                  "multi": "paragraph,strong,em,hyperlink"
                }
              }
            }
          }
        }
      },
      "items": {}
    }
  ]
}
